    private void btnHasilActionPerformed(java.awt.event.ActionEvent evt) {                                         
        txtNama.setText("");
        txtJenis.setText("");
        txtLama.setText("");
        hslNama.setText("");
        hslHarga.setText("");
        hslJenis.setText("");
        totalBayar.setText("");
    }                                        

